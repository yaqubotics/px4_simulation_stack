param set MPC_Z_VEL_MAX_UP 0.5
param set WV_YRATE_MAX 1.0
param set NAV_DLL_ACT 0
param set MPC_ACC_HOR_MAX 2.0
param set MPC_HOLD_MAX_XY 0.2
